{
    "name": "react-vite",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "build": "pnpm --parallel --filter \"./**\" build",
        "serve": "pnpm --parallel --filter \"./**\" preview",
        "build:remotes": "pnpm --parallel --filter \"./remotes\" build",
        "serve:remotes": "pnpm --parallel --filter \"./remotes\" serve",
        "dev:hosts": "pnpm --filter \"./KSM-Host\" dev",
        "stop": "kill-port --port 5000,5001"
    },
    "devDependencies": {
        "kill-port": "^2.0.1",
        "@originjs/vite-plugin-federation": "^1.3.2"
    }
}